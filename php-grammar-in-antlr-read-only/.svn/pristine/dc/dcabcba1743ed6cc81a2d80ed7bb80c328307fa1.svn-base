	private function privateNonStaticFunction() {echo "privateNonStaticFunction";}
	function defaultNonStaticFunction() {echo "defaultNonStaticFunction";}
      
	const CONSTANT_FIELD = "CONSTANT_FIELD";
}


$a = new A();
$a->p|

